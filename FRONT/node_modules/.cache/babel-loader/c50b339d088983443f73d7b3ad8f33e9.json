{"ast":null,"code":"import HeaderView from '@/views/HeaderView.vue';\nexport default {\n  name: 'InscriptionPage',\n  data: () => ({\n    userExists: false,\n    email: '',\n    prenom: '',\n    nom: '',\n    password: '',\n    password_confirm: '',\n    error: false,\n    rules: {\n      required: value => !!value || \"Required\",\n      email: value => {\n        const pattern = \"REGEX\";\n        return pattern.test(value) || \"Email invalide\";\n      }\n    }\n  }),\n  methods: {\n    createAccount() {\n      if (this.valid()) {\n        this.$store.dispatch('REGISTER', {\n          prenom: this.prenom,\n          email: this.email,\n          nom: this.nom,\n          password: this.password\n        }).then(({\n          status\n        }) => {\n          this.$store.commit(\"SET_NOTIFICATION\", {\n            display: true,\n            message: 'Votre compte a bien été créer !',\n            alertClass: \"danger\"\n          });\n          this.$router.push('/login');\n        }).catch(error => {\n          this.userExists = true;\n        });\n      }\n    },\n\n    valid() {\n      return this.password === this.password_confirm;\n    }\n\n  },\n  components: {\n    HeaderView\n  }\n};","map":{"version":3,"mappings":"AA+BA,OAAOA,UAAP,MAAuB,wBAAvB;AAGA,eAAe;EACbC,IAAI,EAAE,iBADO;EAEbC,IAAI,EAAE,OAAO;IACXC,UAAU,EAAE,KADD;IAEXC,KAAK,EAAE,EAFI;IAGXC,MAAM,EAAE,EAHG;IAIXC,GAAG,EAAE,EAJM;IAKXC,QAAQ,EAAE,EALC;IAMXC,gBAAgB,EAAE,EANP;IAOXC,KAAK,EAAE,KAPI;IAQXC,KAAK,EAAE;MACLC,QAAQ,EAAEC,KAAI,IAAK,CAAC,CAACA,KAAF,IAAW,UADzB;MAELR,KAAK,EAAEQ,KAAI,IAAK;QACd,MAAMC,OAAM,GAAI,OAAhB;QACA,OAAOA,OAAO,CAACC,IAAR,CAAaF,KAAb,KAAuB,gBAA9B;MACF;IALK;EARI,CAAP,CAFO;EAkBbG,OAAO,EAAE;IACPC,aAAa,GAAG;MACd,IAAI,KAAKC,KAAL,EAAJ,EAAkB;QAChB,KAAKC,MAAL,CAAYC,QAAZ,CAAqB,UAArB,EAAiC;UAC/Bd,MAAM,EAAE,KAAKA,MADkB;UAE/BD,KAAK,EAAE,KAAKA,KAFmB;UAG/BE,GAAG,EAAE,KAAKA,GAHqB;UAI/BC,QAAQ,EAAE,KAAKA;QAJgB,CAAjC,EAMCa,IAND,CAMM,CAAC;UAAEC;QAAF,CAAD,KAAgB;UACpB,KAAKH,MAAL,CAAYI,MAAZ,CAAmB,kBAAnB,EAA