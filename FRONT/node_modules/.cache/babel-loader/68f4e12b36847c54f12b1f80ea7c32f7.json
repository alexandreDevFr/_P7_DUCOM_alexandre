{"ast":null,"code":"export default {\n  name: 'ModifyComponent',\n  props: ['onClose'],\n\n  data() {\n    return {\n      isNew: false,\n      isModify: false,\n      posts: [],\n      description: '',\n      image: '',\n      imageTest: ''\n    };\n  },\n\n  methods: {\n    modifPost: function (_id) {\n      console.log(_id);\n      const userId = JSON.parse(localStorage.getItem(\"userId\"));\n      const token = JSON.parse(localStorage.getItem(\"token\"));\n      let formData = new FormData();\n      formData.append('userId', userId);\n      formData.append('post', this.description);\n      formData.append('image', this.file);\n      formData.append('prenom', 'Alex');\n      formData.append('nom', 'alex');\n      console.log(_id);\n      console.log('---->1', userId); // Log du Token\n\n      fetch('http://localhost:3000/api/posts/' + `${_id}`, {\n        method: 'PUT',\n        headers: {\n          'Content-Type': 'application/json',\n          'Authorization': `Bearer ${token}`\n        },\n        body: formData\n      }).then(response => {\n        response.json();\n        console.log(response); // this.$router.go()\n      }).catch(error => console.log(error));\n    }\n  },\n\n  mounted() {\n    fetch('http://localhost:3000/api/posts').then(res => res.json()).then(data => this.posts = data).catch(err => console.log(err.message));\n  }\n\n};","map":{"version":3,"mappings":"AAsBA,eAAe;EACXA,IAAI,EAAE,iBADK;EAEXC,KAAK,EAAE,CAAC,SAAD,CAFI;;EAGXC,IAAI,GAAE;IACN,OAAM;MACLC,KAAK,EAAE,KADF;MAELC,QAAQ,EAAE,KAFL;MAGLC,KAAK,EAAE,EAHF;MAILC,WAAW,EAAE,EAJR;MAKLC,KAAK,EAAE,EALF;MAMLC,SAAQ,EAAI;IANP,CAAN;EAQD,CAZY;;EAaXC,OAAO,EAAE;IACPC,SAAS,EAAE,UAAUC,GAAV,EAAe;MAC1BC,OAAO,CAACC,GAAR,CAAYF,GAAZ;MACA,MAAMG,MAAK,GAAIC,IAAI,CAACC,KAAL,CAAWC,YAAY,CAACC,OAAb,CAAqB,QAArB,CAAX,CAAf;MACA,MAAMC,KAAI,GAAIJ,IAAI,CAACC,KAAL,CAAWC,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAX,CAAd;MAEA,IAAIE,QAAO,GAAI,IAAIC,QAAJ,EAAf;MACAD,QAAQ,CAACE,MAAT,CAAgB,QAAhB,EAA0BR,MAA1B;MACAM,QAAQ,CAACE,MAAT,CAAgB,MAAhB